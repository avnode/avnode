extends ../layout

block content
  .card-columns
    each item in data
      .card
        img.card-img-top.img-fluid(
          src=item.imagePath
          alt=item.name
        )
        .card-block
          h4.card-title
            =item.stagename
          if item.citizenship
            p.card-subtitle.mb-3.text-muted
              =item.citizenship
          a.btn.btn-primary(href=item.publicUrl)=__('More')
